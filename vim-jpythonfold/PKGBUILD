pkgname=vim-jpythonfold
pkgver=3.2
pkgrel=4
pkgdesc="A better python fold script"
arch=('i686' 'x86_64')
url="http://www.vim.org/scripts/script.php?script_id=2527"
license=('unknown')
groups=('vim-plugins')
depends=(vim)
source=(http://www.vim.org/scripts/download_script.php?src_id=10034)
md5sums=('81866af3199f166573c5225a9f839678')

build() {
    install -Dm644 ${srcdir}/${pkgname} ${pkgdir}/usr/share/vim/vimfiles/plugin/jpythonfold.vim || return 1
}

